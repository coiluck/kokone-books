{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:window:allow-close",
    "core:path:default",
    "store:allow-load",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save",
    "store:allow-delete",
    "store:default",
    "opener:default",
    "sql:default",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select",
    "sql:allow-close",
    "dialog:default",
    "dialog:allow-save",
    "fs:default",
    "fs:allow-write-text-file",
    "fs:write-all",
    "clipboard-manager:allow-read-text"
  ]
}